  "main": "index.js",
  "scripts": {
    "start": "node index",
    "test": "jest",
    "coverage": "jest --coverage",
    "dist": "cross-env NODE_ENV=production webpack -p --progress",
    "postinstall": "npm run dist"
  },
  "repository": {
    "type": "git"
  },
  "engines": {
    "node": "5",
    "npm": "3"
  },
  "keywords": [
