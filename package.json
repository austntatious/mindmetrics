    "production",
    "sass",
    "hot",
    "reload"
  ],
  "author": "Austin Tse",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "babel-runtime": "6.18.0",
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.1",
    "classnames": "^2.2.5",
    "compression": "1.6.1",
    "connect-mongo": "^1.2.0",
    "cross-env": "1.0.7",
    "d3": "^3.5.17",
    "d3-svg-legend": "^1.12.0",
    "ejs": "2.4.1",
    "express": "4.13.4",
    "express-ejs-layouts": "2.0.0",
    "express-session": "^1.13.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch": "^1.0.1",
    "file-loader": "^0.8.5",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.0",
    "mongoose": "^4.4.17",
    "node-uuid": "^1.4.7",
    "object-assign": "^4.1.0",
    "precss": "^1.4.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-faux-dom": "^2.6.2",
    "react-mixin": "^2.0.2",
    "react-router": "^2.4.0",
    "react-tap-event-plugin": "^1.0.0",
